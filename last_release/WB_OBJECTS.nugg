 <PROG NAME="ZIN_WB_SCANNER_OBJECTS" VARCL="X" DBAPL="S" DBNA="D$" SUBC="I" APPL="S" LEVL="702" RMAND="000" RLOAD="D" FIXPT="X" LDBNAME="D$S">
